{
  "name": "synthetica",
  "version": "0.1.0",
  "description": "Web Audio modules for emulating classic synths",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && copy-files-from-to && rollup -c",
    "serve": "npm run build && serve",
    "clean": "rimraf dist assets/vendor",
    "watch": "rollup -cw",
    "deploy:gh": "gh-pages --dist .",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix --ext ts --ext js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pendragon-andyh/synthetica.git"
  },
  "keywords": [
    "WebAudio"
  ],
  "author": "Andy Harman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pendragon-andyh/synthetica/issues"
  },
  "homepage": "https://github.com/pendragon-andyh/synthetica#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "acorn": "^7.3.1",
    "copy-files-from-to": "^2.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gh-pages": "^3.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.16.1",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "serve": "^11.3.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  },
  "copyFiles": [
    {
      "from": "node_modules/jquery/dist/jquery.slim.min.*",
      "to": "assets/vendor/jquery/"
    }
  ]
}